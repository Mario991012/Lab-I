@model IEnumerable<Lab_1.Models.Trabajadores>

@{
    ViewBag.Title = "Resultados";
}

<h2>Resultados</h2>

<p>
    @Html.ActionLink("Registrar trabajador", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sueldoFinal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Citas)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Llegada)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Salida)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BoolOficina)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sueldoFinal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Citas)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Llegada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Salida)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BoolOficina)
        </td>
    </tr>
}

</table>

<div>
    @Html.ActionLink("Regresar a Menú", "Index")
</div>

